@import "/css/swiper.wxss";
.swiper-tab {
  width: 100%;
  border-bottom: 2rpx solid #777;
  text-align: center;
  line-height: 80rpx;
  position: fixed;
  z-index: 999;
}

.swiper-tab-list {
  font-size: 24rpx;
  width: 25%; 
}

.content {
  width: 90%;
  margin-left: 5%;
  font-size: 26rpx;
  line-height: 60rpx;
}

.th {
  margin-top: 60rpx;
  width: 100%;
  height: 60rpx;
  background-color: #aaa;
  font-size: 26rpx;
  display: inline-block;
  line-height: 60rpx;
}

.th view {
  display: inline-block;
  width: 49.5%;
  height: 60rpx;
  border: 1rpx solid #fff;
  color: #fff;
  text-align: center;
}

